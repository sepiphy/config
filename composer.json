{
    "name": "sepiphy/config",
    "description": "The \"sepiphy/config\" package.",
    "keywords": ["sepiphy", "tools", "phptools", "config"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/sepiphy/phptools",
    "support": {
        "issues": "https://github.com/sepiphy/phptools/issues",
        "source": "https://github.com/sepiphy/phptools"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com",
            "homepage": "https://seriquynh.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "sepiphy/contracts": "dev-master",
        "symfony/finder": "^4.2"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Sepiphy\\PHPTools\\Config\\": "./"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
